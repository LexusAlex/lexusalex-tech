{
  "name": "lexusalex/lexusalex-tech",
  "description": "lexusalex-tech",
  "type": "project",
  "minimum-stability": "stable",
  "license": "BSD-3-Clause",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Alexey Shmelev",
      "email": "alexsey_89@bk.ru"
    }
  ],
  "autoload": {
    "files": [
      "src/Configurations/Main/environment.php"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require": {
    "doctrine/dbal": "^3.7",
    "laminas/laminas-config-aggregator": "^1.14",
    "league/oauth2-server": "^8.5",
    "monolog/monolog": "^3.5",
    "php-di/php-di": "^7.0",
    "ramsey/uuid": "^4.7",
    "robmorgan/phinx": "^0.15.5",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.46",
    "phpunit/phpunit": "^10.5",
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "phpunit": "phpunit --colors=always --configuration=etc/phpunit.xml",
    "php-cs-fixer": "php-cs-fixer fix --allow-risky=yes --config etc/.php-cs-fixer.php --diff",
    "phinx": "phinx"
  }
}
