{
  "name": "lexusalex/lexusalex-tech",
  "description": "lexusalex-tech",
  "type": "project",
  "minimum-stability": "stable",
  "license": "BSD-3-Clause",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Alexey Shmelev",
      "email": "alexsey_89@bk.ru"
    }
  ],
  "autoload": {
    "files": [
      "src/Configurations/Main/environment.php"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require": {
    "laminas/laminas-config-aggregator": "^1.14",
    "php-di/php-di": "^7.0",
    "ramsey/uuid": "^4.7",
    "robmorgan/phinx": "^0.15.5",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "phpunit": "phpunit --colors=always --configuration=etc/phpunit.xml"
  }
}
